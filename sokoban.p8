-- state

function to_clear()
 timer=0
 _upd=upd_clear
 _drw=drw_clear
end

function to_game()
 _upd=upd_game
 _drw=drw_game
end

function to_intro()
 timer=0
 stages[1].mini=stage_mini(1)
 _upd=upd_intro
 _drw=drw_intro
end

function to_selection()
 _upd=upd_stage_selection
 _drw=drw_stage_selection
end

